{
  "name": "react-redux-shopping-list-app",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "materialize-css": "^0.100.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-overlay-loader": "0.0.3",
    "react-redux": "^4.4.8",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "react-toastify": "^3.2.1",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:slapp lint:watch test:watch test:coverage",
    "serve:slapp": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js ",
    "build": "babel src -d lib",
    "test": "nyc jest",
    "test:coverage": "npm run test -- --coverage && coveralls",
    "test:watch": "npm run test -- --watchAll",
    "test:watch:coverage": "npm ",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "node_modules/.bin/esw webpack.config.* src public/assets/js",
    "lint:watch": "npm run lint -- --watch",
    "eject": "react-scripts eject"
  },
  "nyc": {
    "exclude": [
      "src/jestSetup.js",
      "src/**/*.test.js"
    ]
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./src/jestSetup.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-watch": "^3.1.3",
    "expect": "^22.0.3",
    "jest": "^22.0.4",
    "jest-enzyme": "^4.0.2",
    "jsdom": "^11.5.1",
    "mock-local-storage": "^1.0.5",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "keywords": [
    "react",
    "redux",
    "react-component",
    "material design",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:parseendavid/shopping-list-react-app.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parseendavid/shopping-list-react-app/issues"
  }
}
