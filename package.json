{
  "name": "react-redux-shopping-list-app",
  "version": "1.0.0",
  "babel": {
    "presets": [
      "env"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  },
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "lodash": "^4.17.4",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^4.4.8",
    "react-redux-form": "^1.16.3",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-thunk": "^2.2.0"
  },
  "scripts": {
    "start": "npm-run-all --parallel serve:slapp",
    "serve:slapp": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js ",
    "build": "babel src -d lib",
    "test": "react-scripts test --env=jsdom",
    "lint": "node_modules/.bin/esw webpack.config.* src public/assets/js",
    "lint:watch": "npm run lint -- --watch",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint-watch": "^3.1.3",
    "expect": "^22.0.3",
    "jsdom": "^11.5.1",
    "npm-run-all": "^4.1.2",
    "redux-devtools-extension": "^2.13.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  },
  "keywords": [
    "react",
    "redux",
    "react-component",
    "material design",
    "material-ui"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:parseendavid/shopping-list-react-app.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/parseendavid/shopping-list-react-app/issues"
  }
}
